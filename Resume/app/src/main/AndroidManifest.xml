<?xml version="1.0" encoding="utf-8"?>
<manifest package="com.itheima.resume"
          xmlns:android="http://schemas.android.com/apk/res/android">

    <uses-permission android:name="android.permission.INTERNET"></uses-permission>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme.AnimationActivity">
        <activity android:name=".MainActivity"
            >
        </activity>
        <activity android:name=".activity.SplashActivity"
                  android:theme="@style/AppTheme.FullScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>

        </activity>
        <activity android:name=".activity.AboutActivity"></activity>
        <activity android:name=".activity.MyAboutActivity"></activity>
        <activity android:name=".activity.WebViewActivity"></activity>
        <activity android:name=".activity.DetailsActivity"></activity>
    </application>

</manifest>